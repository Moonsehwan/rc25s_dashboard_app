===== ai-react-loop.service =====
[Unit]
Description=ai-react-loop - VibeCoding AGI Service
After=network.target

[Service]
Type=simple
ExecStart=/srv/repo/vibecoding/rc25h_env/bin/python -m vibecoding.ai_react_autoloop
WorkingDirectory=/srv/repo/vibecoding
Restart=always
EnvironmentFile=/etc/vibecoding/env
StandardOutput=append:/srv/repo/vibecoding/logs/ai-react-loop.log
StandardError=append:/srv/repo/vibecoding/logs/ai-react-loop_error.log

[Install]
WantedBy=multi-user.target



