

===== agi-memory.service =====
[Unit]
Description=agi-memory.service - VibeCoding AGI Service
After=network.target

[Service]
Type=simple
WorkingDirectory=/srv/repo/vibecoding
Environment=PYTHONPATH=/srv/repo/vibecoding
ExecStart=/srv/repo/vibecoding/rc25h_env/bin/python -m vibecoding.agi-memory
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target



